{
  "name": "@carbonplan/layouts",
  "version": "1.0.0-develop.22",
  "description": "page layout components for reusable content",
  "type": "module",
  "main": "dst/index.cjs",
  "module": "dst/index.module.js",
  "source": "src/index.js",
  "scripts": {
    "build": "rimraf dst && microbundle build --jsx React.createElement --jsxFragment React.Fragment -f modern,es,cjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carbonplan/layouts.git"
  },
  "author": "freeman-lab",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/carbonplan/layouts/issues"
  },
  "files": [
    "pubs/dst",
    "docs/dst",
    "dst",
    "README.md",
    "package.json"
  ],
  "prettier": {
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "printWidth": 80,
    "quoteProps": "as-needed",
    "jsxSingleQuote": true
  },
  "homepage": "https://github.com/carbonplan/layouts#readme",
  "exports": {
    ".": {
      "require": "./dst/index.cjs",
      "default": "./dst/index.modern.js"
    }
  },
  "dependencies": {
    "@carbonplan/components": "^11.0.6",
    "@carbonplan/icons": "^1.1.0"
  },
  "peerDependencies": {
    "react": "^16.14.0 || ^17.0.2",
    "react-dom": "^16.14.0 || ^17.0.2",
    "theme-ui": ">=0.11.0"
  },
  "devDependencies": {
    "microbundle": "^0.14.0",
    "prettier": "2.5.0",
    "rimraf": "2.6.3",
    "npm-run-all": "^4.0.0"
  }
}
